    "version": "0.2.0",
    "configurations": [
        {
            "name": "Demeter Dev",
            "request": "launch",
            "type": "dart",
            "program": "demeter_app//lib/main-dev.dart",
            "args": [
                "--flavor dev"
            ]
        },
        {
            "name": "Demeter Prod",
            "request": "launch",
            "type": "dart",
            "program": "demeter_app//lib/main-prod.dart",
            "args": [
                "--flavor prod"
            ]
        },
        {
            "name": "demeter_app",
            "cwd": "demeter_app",
            "request": "launch",
            "type": "dart",
            
        },
        {
            "name": "demeter_app (profile mode)",
            "cwd": "demeter_app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "demeter_app (release mode)",
            "cwd": "demeter_app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
       
    ]
}